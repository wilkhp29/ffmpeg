{
  "name": "ffmpeg-worker-render",
  "version": "1.0.0",
  "description": "FFmpeg Worker (URL mode) em TypeScript para Render (Docker Web Service)",
  "main": "server.ts",
  "type": "commonjs",
  "packageManager": "bun@1.2.22",
  "scripts": {
    "start": "tsx server.ts",
    "start:bun": "bun server.ts",
